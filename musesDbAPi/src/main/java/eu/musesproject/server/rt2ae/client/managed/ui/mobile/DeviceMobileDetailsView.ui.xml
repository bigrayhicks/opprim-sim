<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
  	
    <div id="deviceId">
            <div class="{style.label}">Device Id</div>
            <div class="{style.value}" ui:field="deviceId"/>
        </div>
    
    <div id="accessrequests">
            <div class="{style.label}">Accessrequests</div>
            <div class="{style.value}" ui:field="accessrequests"/>
        </div>
    
    <div id="deviceSecurityStates">
            <div class="{style.label}">Device Security States</div>
            <div class="{style.value}" ui:field="deviceSecurityStates"/>
        </div>
    
    <div id="securityIncidents">
            <div class="{style.label}">Security Incidents</div>
            <div class="{style.value}" ui:field="securityIncidents"/>
        </div>
    
    <div id="ownerId">
            <div class="{style.label}">Owner Id</div>
            <div class="{style.value}" ui:field="ownerId"/>
        </div>
    
    <div id="name">
            <div class="{style.label}">Name</div>
            <div class="{style.value}" ui:field="name"/>
        </div>
    
    <div id="type">
            <div class="{style.label}">Type</div>
            <div class="{style.value}" ui:field="type"/>
        </div>
    
    <div id="description">
            <div class="{style.label}">Description</div>
            <div class="{style.value}" ui:field="description"/>
        </div>
    
    <div id="imei">
            <div class="{style.label}">Imei</div>
            <div class="{style.value}" ui:field="imei"/>
        </div>
    
    <div id="macaddress">
            <div class="{style.label}">Macaddress</div>
            <div class="{style.value}" ui:field="macaddress"/>
        </div>
    
    <div id="os">
            <div class="{style.label}">Os</div>
            <div class="{style.value}" ui:field="os"/>
        </div>
    
    <div id="trustvalue">
            <div class="{style.label}">Trustvalue</div>
            <div class="{style.value}" ui:field="trustvalue"/>
        </div>
    
    <div id="version">
            <div class="{style.label}">Version</div>
            <div class="{style.value}" ui:field="version"/>
        </div>
    

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
