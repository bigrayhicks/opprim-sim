<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:a="urn:import:eu.musesproject.server.rt2ae.client.managed.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client" xmlns:e="urn:import:eu.musesproject.server.rt2ae.client.managed.ui.editor" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:r="urn:import:eu.musesproject.server.rt2ae.client.scaffold.ui">
  <ui:style>
    .outer {
      padding: 10px;
    }
    .error {
      padding-left: 0.5em;
      background-color: red;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding-bottom: 10px;
    }
  </ui:style>
 
  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
    <div class="{style.error}" ui:field="errors"/>
    
    
    
    
    
    
    
    

    <g:Button ui:field="save">Save</g:Button>
  
        
        
        
        
        
        
    
        
        
        
        
        
        
    
        
        
        
        
        
        
    
        
        
        
        
        
        
    <div id="assetsId">
            <div class="{style.label}">Assets Id</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="assetsId"/>
            </div>
        </div>
        <div id="devicesId">
            <div class="{style.label}">Devices Id</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="devicesId"/>
            </div>
        </div>
        <div id="usersId">
            <div class="{style.label}">Users Id</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="usersId"/>
            </div>
        </div>
        <div id="description">
            <div class="{style.label}">Description</div>
            <div class="{style.value}">
                <g:TextBox ui:field="description"/>
            </div>
        </div>
        <div id="costbenefit">
            <div class="{style.label}">Costbenefit</div>
            <div class="{style.value}">
                <g:DoubleBox ui:field="costbenefit"/>
            </div>
        </div>
        <div id="decisionId">
            <div class="{style.label}">Decision Id</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="decisionId"/>
            </div>
        </div>
        <div id="probability">
            <div class="{style.label}">Probability</div>
            <div class="{style.value}">
                <g:DoubleBox ui:field="probability"/>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
