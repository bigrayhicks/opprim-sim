<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .fields {
      margin-top: 0.5em;
      margin-left: 1em;
    }
    .label {
      min-height: 25px;
      font-weight: bold;
    }
    .button {
      margin-right: 1em;
    }
    .bar {
      margin-left: 1em;
    }
    .header {
      margin-left: 1em;
      color: #4B4A4A;
      text-shadow: #ddf 1px 1px 0;
      margin-bottom: 0;
    }
    .underline {
      border-bottom: 2px solid #6F7277;
    }
  </ui:style>
  <g:HTMLPanel>
    <div class="{style.underline}">
      <h3 class="{style.header}">
                <ui:msg>
                    <span ui:field="displayRenderer"/>
                </ui:msg>
            </h3>
    </div>

    <table class="{style.fields}" id="boundElementHolder">
      
      <tr id="deviceId">
                <td>
                    <span class="{style.label}">Device Id:</span>
                </td>
                <td>
                    <span ui:field="deviceId"/>
                </td>
            </tr>
      
      <tr id="accessrequests">
                <td>
                    <span class="{style.label}">Accessrequests:</span>
                </td>
                <td>
                    <span ui:field="accessrequests"/>
                </td>
            </tr>
      
      <tr id="deviceSecurityStates">
                <td>
                    <span class="{style.label}">Device Security States:</span>
                </td>
                <td>
                    <span ui:field="deviceSecurityStates"/>
                </td>
            </tr>
      
      <tr id="securityIncidents">
                <td>
                    <span class="{style.label}">Security Incidents:</span>
                </td>
                <td>
                    <span ui:field="securityIncidents"/>
                </td>
            </tr>
      
      <tr id="ownerId">
                <td>
                    <span class="{style.label}">Owner Id:</span>
                </td>
                <td>
                    <span ui:field="ownerId"/>
                </td>
            </tr>
      
      <tr id="name">
                <td>
                    <span class="{style.label}">Name:</span>
                </td>
                <td>
                    <span ui:field="name"/>
                </td>
            </tr>
      
      <tr id="type">
                <td>
                    <span class="{style.label}">Type:</span>
                </td>
                <td>
                    <span ui:field="type"/>
                </td>
            </tr>
      
      <tr id="description">
                <td>
                    <span class="{style.label}">Description:</span>
                </td>
                <td>
                    <span ui:field="description"/>
                </td>
            </tr>
      
      <tr id="imei">
                <td>
                    <span class="{style.label}">Imei:</span>
                </td>
                <td>
                    <span ui:field="imei"/>
                </td>
            </tr>
      
      <tr id="macaddress">
                <td>
                    <span class="{style.label}">Macaddress:</span>
                </td>
                <td>
                    <span ui:field="macaddress"/>
                </td>
            </tr>
      
      <tr id="os">
                <td>
                    <span class="{style.label}">Os:</span>
                </td>
                <td>
                    <span ui:field="os"/>
                </td>
            </tr>
      
      <tr id="trustvalue">
                <td>
                    <span class="{style.label}">Trustvalue:</span>
                </td>
                <td>
                    <span ui:field="trustvalue"/>
                </td>
            </tr>
      
      <tr id="version">
                <td>
                    <span class="{style.label}">Version:</span>
                </td>
                <td>
                    <span ui:field="version"/>
                </td>
            </tr>
      
    </table>

  <div class="{style.bar}">
    <g:Button addStyleNames="{style.button}" ui:field="edit">Edit</g:Button>
    <g:Anchor ui:field="delete">Delete</g:Anchor>
   </div>
  </g:HTMLPanel>
</ui:UiBinder>
