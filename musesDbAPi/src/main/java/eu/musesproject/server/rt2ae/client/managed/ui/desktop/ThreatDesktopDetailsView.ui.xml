<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .fields {
      margin-top: 0.5em;
      margin-left: 1em;
    }
    .label {
      min-height: 25px;
      font-weight: bold;
    }
    .button {
      margin-right: 1em;
    }
    .bar {
      margin-left: 1em;
    }
    .header {
      margin-left: 1em;
      color: #4B4A4A;
      text-shadow: #ddf 1px 1px 0;
      margin-bottom: 0;
    }
    .underline {
      border-bottom: 2px solid #6F7277;
    }
  </ui:style>
  <g:HTMLPanel>
    <div class="{style.underline}">
      <h3 class="{style.header}">
                <ui:msg>
                    <span ui:field="displayRenderer"/>
                </ui:msg>
            </h3>
    </div>

    <table class="{style.fields}" id="boundElementHolder">
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
    <tr id="threatId">
                <td>
                    <span class="{style.label}">Threat Id:</span>
                </td>
                <td>
                    <span ui:field="threatId"/>
                </td>
            </tr>
            <tr id="accessrequests">
                <td>
                    <span class="{style.label}">Accessrequests:</span>
                </td>
                <td>
                    <span ui:field="accessrequests"/>
                </td>
            </tr>
            <tr id="clues">
                <td>
                    <span class="{style.label}">Clues:</span>
                </td>
                <td>
                    <span ui:field="clues"/>
                </td>
            </tr>
            <tr id="outcomes">
                <td>
                    <span class="{style.label}">Outcomes:</span>
                </td>
                <td>
                    <span ui:field="outcomes"/>
                </td>
            </tr>
            <tr id="description">
                <td>
                    <span class="{style.label}">Description:</span>
                </td>
                <td>
                    <span ui:field="description"/>
                </td>
            </tr>
            <tr id="probability">
                <td>
                    <span class="{style.label}">Probability:</span>
                </td>
                <td>
                    <span ui:field="probability"/>
                </td>
            </tr>
            <tr id="occurences">
                <td>
                    <span class="{style.label}">Occurences:</span>
                </td>
                <td>
                    <span ui:field="occurences"/>
                </td>
            </tr>
            <tr id="badOutcomeCount">
                <td>
                    <span class="{style.label}">Bad Outcome Count:</span>
                </td>
                <td>
                    <span ui:field="badOutcomeCount"/>
                </td>
            </tr>
            <tr id="ttl">
                <td>
                    <span class="{style.label}">Ttl:</span>
                </td>
                <td>
                    <span ui:field="ttl"/>
                </td>
            </tr>
        </table>

  <div class="{style.bar}">
    <g:Button addStyleNames="{style.button}" ui:field="edit">Edit</g:Button>
    <g:Anchor ui:field="delete">Delete</g:Anchor>
   </div>
  </g:HTMLPanel>
</ui:UiBinder>
