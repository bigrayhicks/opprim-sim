<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
  	
    <div id="riskpolicyId">
            <div class="{style.label}">Riskpolicy Id</div>
            <div class="{style.value}" ui:field="riskpolicyId"/>
        </div>
    
    <div id="riskvalue">
            <div class="{style.label}">Riskvalue</div>
            <div class="{style.value}" ui:field="riskvalue"/>
        </div>
    
    <div id="description">
            <div class="{style.label}">Description</div>
            <div class="{style.value}" ui:field="description"/>
        </div>
    

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
