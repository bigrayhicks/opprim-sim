<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:a="urn:import:eu.musesproject.server.rt2ae.client.managed.ui" xmlns:d="urn:import:com.google.gwt.user.datepicker.client" xmlns:e="urn:import:eu.musesproject.server.rt2ae.client.managed.ui.editor" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:r="urn:import:eu.musesproject.server.rt2ae.client.scaffold.ui">
  <ui:style>
    .outer {
      padding: 10px;
    }
    .error {
      padding-left: 0.5em;
      background-color: red;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding-bottom: 10px;
    }
  </ui:style>
 
  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
    <div class="{style.error}" ui:field="errors"/>
    
    
    
    
    
    
    
    
    
    
    

    <g:Button ui:field="save">Save</g:Button>
  <div id="assets">
            <div class="{style.label}">Assets</div>
            <div class="{style.value}">
                <e:AssetSetEditor ui:field="assets"/>
            </div>
        </div>
        <div id="riskinformations">
            <div class="{style.label}">Riskinformations</div>
            <div class="{style.value}">
                <e:RiskinformationSetEditor ui:field="riskinformations"/>
            </div>
        </div>
        <div id="opportunityId">
            <div class="{style.label}">Opportunity Id</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="opportunityId"/>
            </div>
        </div>
        <div id="deviceId">
            <div class="{style.label}">Device Id</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="deviceId"/>
            </div>
        </div>
        <div id="userId">
            <div class="{style.label}">User Id</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="userId"/>
            </div>
        </div>
        <div id="riskcommunicationid">
            <div class="{style.label}">Riskcommunicationid</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="riskcommunicationid"/>
            </div>
        </div>
        <div id="threatid">
            <div class="{style.label}">Threatid</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="threatid"/>
            </div>
        </div>
        <div id="useractionId">
            <div class="{style.label}">Useraction Id</div>
            <div class="{style.value}">
                <g:ValueListBox ui:field="useractionId"/>
            </div>
        </div>
        <div id="solved">
            <div class="{style.label}">Solved</div>
            <div class="{style.value}">
                <r:ShortBox ui:field="solved"/>
            </div>
        </div>
        <div id="riskcommunicationId">
            <div class="{style.label}">Riskcommunication Id</div>
            <div class="{style.value}">
                <g:IntegerBox ui:field="riskcommunicationId"/>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
