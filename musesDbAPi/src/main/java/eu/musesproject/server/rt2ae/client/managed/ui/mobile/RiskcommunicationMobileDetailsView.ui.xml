<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
  	
    <div id="riskcommunicationId">
            <div class="{style.label}">Riskcommunication Id</div>
            <div class="{style.value}" ui:field="riskcommunicationId"/>
        </div>
    
    <div id="accessrequests">
            <div class="{style.label}">Accessrequests</div>
            <div class="{style.value}" ui:field="accessrequests"/>
        </div>
    
    <div id="decisions">
            <div class="{style.label}">Decisions</div>
            <div class="{style.value}" ui:field="decisions"/>
        </div>
    
    <div id="risktreatments">
            <div class="{style.label}">Risktreatments</div>
            <div class="{style.value}" ui:field="risktreatments"/>
        </div>
    
    <div id="textualDescription">
            <div class="{style.label}">Textual Description</div>
            <div class="{style.value}" ui:field="textualDescription"/>
        </div>
    

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
