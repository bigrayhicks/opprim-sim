<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
  	
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    

    <g:Button ui:field="delete">Delete</g:Button>
  <div id="userId">
            <div class="{style.label}">User Id</div>
            <div class="{style.value}" ui:field="userId"/>
        </div>
        <div id="accessrequests">
            <div class="{style.label}">Accessrequests</div>
            <div class="{style.value}" ui:field="accessrequests"/>
        </div>
        <div id="devices">
            <div class="{style.label}">Devices</div>
            <div class="{style.value}" ui:field="devices"/>
        </div>
        <div id="securityIncidents">
            <div class="{style.label}">Security Incidents</div>
            <div class="{style.value}" ui:field="securityIncidents"/>
        </div>
        <div id="name">
            <div class="{style.label}">Name</div>
            <div class="{style.value}" ui:field="name"/>
        </div>
        <div id="surname">
            <div class="{style.label}">Surname</div>
            <div class="{style.value}" ui:field="surname"/>
        </div>
        <div id="email">
            <div class="{style.label}">Email</div>
            <div class="{style.value}" ui:field="email"/>
        </div>
        <div id="trustvalue">
            <div class="{style.label}">Trustvalue</div>
            <div class="{style.value}" ui:field="trustvalue"/>
