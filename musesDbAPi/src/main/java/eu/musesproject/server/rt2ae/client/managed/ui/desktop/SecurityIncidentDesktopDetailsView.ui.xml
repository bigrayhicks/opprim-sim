<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .fields {
      margin-top: 0.5em;
      margin-left: 1em;
    }
    .label {
      min-height: 25px;
      font-weight: bold;
    }
    .button {
      margin-right: 1em;
    }
    .bar {
      margin-left: 1em;
    }
    .header {
      margin-left: 1em;
      color: #4B4A4A;
      text-shadow: #ddf 1px 1px 0;
      margin-bottom: 0;
    }
    .underline {
      border-bottom: 2px solid #6F7277;
    }
  </ui:style>
  <g:HTMLPanel>
    <div class="{style.underline}">
      <h3 class="{style.header}">
                <ui:msg>
                    <span ui:field="displayRenderer"/>
                </ui:msg>
            </h3>
    </div>

    <table class="{style.fields}" id="boundElementHolder">
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
    
            
            
            
            
            
            
            
        <tr id="securityIncidentId">
                <td>
                    <span class="{style.label}">Security Incident Id:</span>
                </td>
                <td>
                    <span ui:field="securityIncidentId"/>
                </td>
            </tr>
            <tr id="assetsId">
                <td>
                    <span class="{style.label}">Assets Id:</span>
                </td>
                <td>
                    <span ui:field="assetsId"/>
                </td>
            </tr>
            <tr id="devicesId">
                <td>
                    <span class="{style.label}">Devices Id:</span>
                </td>
                <td>
                    <span ui:field="devicesId"/>
                </td>
            </tr>
            <tr id="usersId">
                <td>
                    <span class="{style.label}">Users Id:</span>
                </td>
                <td>
                    <span ui:field="usersId"/>
                </td>
            </tr>
            <tr id="description">
                <td>
                    <span class="{style.label}">Description:</span>
                </td>
                <td>
                    <span ui:field="description"/>
                </td>
            </tr>
            <tr id="costbenefit">
                <td>
                    <span class="{style.label}">Costbenefit:</span>
                </td>
                <td>
                    <span ui:field="costbenefit"/>
                </td>
            </tr>
            <tr id="decisionId">
                <td>
                    <span class="{style.label}">Decision Id:</span>
                </td>
                <td>
                    <span ui:field="decisionId"/>
                </td>
            </tr>
            <tr id="probability">
                <td>
                    <span class="{style.label}">Probability:</span>
                </td>
                <td>
                    <span ui:field="probability"/>
                </td>
            </tr>
        </table>

  <div class="{style.bar}">
    <g:Button addStyleNames="{style.button}" ui:field="edit">Edit</g:Button>
    <g:Anchor ui:field="delete">Delete</g:Anchor>
   </div>
  </g:HTMLPanel>
</ui:UiBinder>
