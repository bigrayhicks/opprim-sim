<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
 <ui:style>
    .outer {
      padding: 10px;
    }
    .label {
      font-weight: bold;
    }
    .value {
      padding: 4px 0px 15px 15px;
    }
  </ui:style>

  <g:HTMLPanel debugId="boundElementHolder" styleName="{style.outer}">
  	
    <div id="riskinformationId">
            <div class="{style.label}">Riskinformation Id</div>
            <div class="{style.value}" ui:field="riskinformationId"/>
        </div>
    
    <div id="accessrequestId">
            <div class="{style.label}">Accessrequest Id</div>
            <div class="{style.value}" ui:field="accessrequestId"/>
        </div>
    
    <div id="assetId">
            <div class="{style.label}">Asset Id</div>
            <div class="{style.value}" ui:field="assetId"/>
        </div>
    
    <div id="type">
            <div class="{style.label}">Type</div>
            <div class="{style.value}" ui:field="type"/>
        </div>
    
    <div id="description">
            <div class="{style.label}">Description</div>
            <div class="{style.value}" ui:field="description"/>
        </div>
    
    <div id="probability">
            <div class="{style.label}">Probability</div>
            <div class="{style.value}" ui:field="probability"/>
        </div>
    

    <g:Button ui:field="delete">Delete</g:Button>
  </g:HTMLPanel>
</ui:UiBinder>
